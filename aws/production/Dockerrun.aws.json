{
    "AWSEBDockerrunVersion": "2",
    "volumes": [
        {
            "name": "app",
            "host": {
                "sourcePath": "/var/app/current/app"
            }
        }
    ],
    "containerDefinitions": [
        {
            "name": "app-container",
            "image": "761633760558.dkr.ecr.us-west-2.amazonaws.com/resin-prometheus-server:production",
            "essential": true,
            "privileged": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "app",
                    "containerPath": "/var/app"
                },
                {
                    "sourceVolume": "awseb-logs-app-container",
                    "containerPath": "/var/log/app"
                }
            ]
        }
    ]
}
